.box-list-item {
  display: -webkit-box;
  position: relative;
}
.box-list-item .list-item-img {
  display: inline-block;
  text-align: center;
  width: 40px;
  height: auto;
  margin-right: 15px;
}
.box-list-item  .list-item-img image {
  width: 40px;
  height: 40px;
  border-radius: 100%;
  border: 1px solid #f5f5f5;
}
.box-list-item .list-item-txt {
  height: auto;
  -webkit-box-flex: 1;
  position: relative;
  text-align: left;
}
.box-list-item .list-item-txt view.view {
  font-size: 14px;
  color: #303030;
