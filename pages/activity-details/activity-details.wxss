  top: 2px;
  right: 0;
   background-color: #fae34c;
  color: #333;
  padding: 2px 10px;
  border-radius: 4px;
}
.activity-notes{
  margin-top: 10px;
  padding: 15px;
  background-color: white;
  box-sizing: border-box;
}
.activity-notes-title{
  text-align: center;
}
.activity-notes-title .title{
  font-size: 18px;
  color: #303030;
  font-weight: 600;
  position: relative;
  display: inline-block;
}
.activity-notes-title .title::before{
  display: block;
    content: "";
    width: 10px;
    height: 1px;
    background-color: #303030;
    position: absolute;
    left: -20px;
    top: 50%;
}
.activity-notes-title .title::after{
  display: block;
    content: "";
    width: 10px;
    height: 1px;
    background-color: #303030;
    position: absolute;
    right: -20px;
    top: 50%;
}
